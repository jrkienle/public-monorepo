{
  "name": "@jrkienle/config",
  "version": "0.1.0",
  "description": "Shared configuration files between all of the @jrkienle monorepo",
  "type": "module",
  "author": "James Kienle (https://www.jameskienle.com)",
  "license": "MIT",
  "keywords": [
    "eslintrc",
    "jrkienle",
    "nextjs",
    "prettierrc",
    "tsconfig"
  ],
  "files": [
    "eslint/**",
    "nextjs/**",
    "prettier/**",
    "typescript/**"
  ],
  "scripts": {
    "reset": "rm -r node_modules"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^13.4.19",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "next": "^13.4.19",
    "prettier": "3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "eslint": ">= 8.47.0 < 9",
    "prettier": "3.0.2",
    "typescript": ">= 5.1.6 < 6"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}

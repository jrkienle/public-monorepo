{
  "name": "@jrkienle/home-management",
  "version": "0.1.0",
  "description": "A dashboard for managing the day to day operations of my home",
  "private": true,
  "type": "module",
  "author": "James Kienle (https://www.jameskienle.com)",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run generate:prisma && next build",
    "clean": "rm -r .next & rm -r .turbo & rm -r coverage & rm -r test-results & rm .eslintcache & rm next.env.d.ts & rm -r src/generated",
    "dev": "next dev",
    "generate:prisma": "prisma generate",
    "lint": "pnpm run generate:prisma && eslint --cache ./src",
    "reset": "rm -r node_modules",
    "start": "next start",
    "test": "pnpm run generate:prisma && vitest"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.23.2",
    "@graphql-yoga/plugin-disable-introspection": "^1.0.4",
    "@jrkienle/ui": "workspace:^",
    "@pothos/core": "^3.33.0",
    "@pothos/plugin-prisma": "^3.57.0",
    "@pothos/plugin-prisma-utils": "^0.11.0",
    "@pothos/plugin-scope-auth": "^3.20.0",
    "@prisma/client": "5.2.0",
    "graphql": "^16.8.0",
    "graphql-scalars": "^1.22.2",
    "graphql-yoga": "^4.0.4",
    "next": "^13.4.19",
    "prisma-extension-pagination": "^0.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@jrkienle/config": "workspace:^",
    "@testing-library/react": "^14.0.0",
    "@types/node": "20.5.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitest/coverage-v8": "^0.34.2",
    "eslint": "^8.47.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.2",
    "prisma": "^5.2.0",
    "vitest": "^0.34.2"
  }
}
